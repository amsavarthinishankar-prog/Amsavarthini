class Main {
    public static void main(String[] args) {
        int num = 5;
        
        for (int i = 1; i <= num; i++) {
            for(int j =1 ; j<= i;j++){
                System.out.print((j % 2)+" ");
            }
            
                System.out.println();
                
            }
           
        }
    }
