class Main {
    public static void main(String[] args) {
        String a = "new world";
        String b = "Amsavarthini";
        String x = "Shankar Kumar";
        String y = "new world";
        System.out.println("Hello".matches(x));
        System.out.println("world".matches(x));
        System.out.println("javascript".matches(x));
        System.out.println(y.regionMatches(4,a,4,5));
        
    }
}
OUTPUT:
false
false
false
true
