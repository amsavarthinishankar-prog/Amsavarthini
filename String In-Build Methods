 PROGRAM 15:       
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner data = new Scanner(System.in);
        System.out.print("Enter the gmail:");
        String gmail = data.nextLine();
        int atIndex = gmail.indexOf('@');
        String domainOnly = gmail.substring(atIndex);
        System.out.println("Result:" + domainOnly);
       
        
    }
}

OUTPUT:
Enter the gmail:amsa@gmail.com
Result:@gmail.com
    }
}
OUTPUT:
true
false
