import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in); 
         String correctUsername = "Amsa__Shankar";
        String correctPassword = "Amsa@123";
        System.out.print("Enter  Your Instagram Id: ");
        String username = input.nextLine();
        System.out.print("Enter  Your Id password: ");
        String password = input.nextLine();
        if (username.equals(correctUsername)) {
            if (password.equals(correctPassword)) {
                System.out.println("Welcome to Instagram Feed!");
            } 
            else
            {
                System.out.println(" password Is Invalid.");
            }
        } 
        else 
        {
            System.out.println("Username Is Not Found.");
        }
     }
}
output:
Enter  Your Instagram Id: Amsa__Shankar
Enter  Your Id password: Amsa@123
Welcome to Instagram Feed!
