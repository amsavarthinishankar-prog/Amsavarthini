class Main {
    public static void main(String[] args) {
        int num = 145;
        int fact = 1;
        int i = 1;

        while (fact < num) {
            i++;
            fact *= i;
        }

        if (fact == num) {
            System.out.println(num + " is a factorial of " + i);
        } else {
            System.out.println(num + " is NOT a factorial.");
        }
    }
}
